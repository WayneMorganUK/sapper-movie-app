{
  "name": "sapper-movie-app",
  "description": "Movie & Tv app",
  "version": "0.0.1",
  "scripts": {
    "dev": "run-p watch:*",
    "watch:css": "postcss src/assets/global.pcss -o static/global.css -w",
    "watch:dev": "sapper dev",
    "build": "run-s build:css build:sapper",
    "build:css": "NODE_ENV=production postcss src/assets/global.pcss -o static/global.css",
    "build:sapper": "sapper build",
    "build:export": "sapper export",
    "export": "run-s build:css build:export",
    "start": "node __sapper__/build",
    "serve": "serve ___sapper__/export",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "start:dev": "sapper dev",
    "validate": "svelte-check --ignore src/node_modules/@sapper"
  },
  "dependencies": {
    "@tailwindcss/line-clamp": "^0.2.0",
    "compression": "^1.7.4",
    "polka": "^1.0.0-next.11",
    "sapper-environment": "^1.0.1",
    "sirv": "^1.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/runtime": "^7.13.10",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^8.2.1",
    "@rollup/plugin-url": "^6.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/node": "^14.14.37",
    "@types/polka": "^0.5.2",
    "autoprefixer": "^10.2.5",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.9",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "postcss-load-config": "^3.0.1",
    "postcss-nested": "^5.0.5",
    "postcss-nesting": "^7.0.1",
    "postcss-preset-env": "^6.7.0",
    "rollup": "^2.44.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.29.1",
    "svelte": "^3.36.0",
    "svelte-check": "^1.3.0",
    "svelte-preprocess": "^4.7.0",
    "tailwindcss": "^2.0.4",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "optionalDependencies": {
    "win-node-env": "^0.4.0"
  }
}
